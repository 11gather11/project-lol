{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"dev": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", ".output/**"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"],
			"outputs": ["dist/**"]
		},
		"biome:check": {
			"dependsOn": ["^biome:check"]
		}
	}
}

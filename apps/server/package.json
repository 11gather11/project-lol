{
	"name": "@project-lol/server",
	"version": "0.1.7",
	"private": true,
	"type": "module",
	"exports": {
		".": "./src/index.js"
	},
	"scripts": {
		"biome:check": "biome check --write",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings",
		"deploy": "npm run generate && npm run remote:migration && wrangler deploy --minify",
		"dev": "wrangler dev",
		"generate": "drizzle-kit generate",
		"local:migration": "wrangler d1 migrations apply project-lol --local",
		"remote:migration": "wrangler d1 migrations apply project-lol --remote",
		"typecheck": "tsc --build"
	},
	"dependencies": {
		"@hono/swagger-ui": "0.5.2",
		"@hono/zod-validator": "0.7.3",
		"drizzle-orm": "0.44.5",
		"drizzle-zod": "0.8.3",
		"hono": "catalog:hono",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"drizzle-kit": "0.31.5",
		"typescript": "catalog:",
		"wrangler": "catalog:"
	}
}

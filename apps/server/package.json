{
	"name": "@project-lol/server",
	"version": "0.1.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy --minify",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings",
		"type-checks": "npm run cf-typegen && tsc --noEmit",
		"generate": "drizzle-kit generate",
		"local:migration": "wrangler d1 migrations apply project-lol --local",
		"remote:migration": "wrangler d1 migrations apply project-lol --remote"
	},
	"exports": {
		".": "./src/index.js"
	},
	"dependencies": {
		"@hono/swagger-ui": "0.5.2",
		"@hono/zod-validator": "0.7.2",
		"drizzle-orm": "0.44.5",
		"drizzle-zod": "0.8.3",
		"hono": "4.9.4",
		"zod": "4.1.5"
	},
	"devDependencies": {
		"drizzle-kit": "0.31.4",
		"wrangler": "4.33.1"
	}
}
